# Example environment variables for Nextcloud Storage Service
# Copy to .env and update values as needed

# Nextcloud DB
POSTGRES_HOST=nextcloud_db
POSTGRES_PORT=5432
POSTGRES_DB=nextcloud
POSTGRES_USER=nextcloud
POSTGRES_PASSWORD=examplepassword

# Nextcloud App
NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=exampleadminpassword
NEXTCLOUD_TRUSTED_DOMAINS=example.com

# Host URLs
NEXTCLOUD_HOST=example.com
